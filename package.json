{
    "name": "dedupe-graphemes",
    "version": "1.0.0",
    "homepage": "https://alexstevovich.com/a/dedupe-graphemes-nodejs",
    "author": {
        "name": "Alex Stevovich",
        "email": "dev@alexstevovich.com",
        "url": "https://alexstevovich.com"
    },
    "description": "Removes duplicate graphemes from a string, preserving order",
    "type": "module",
    "main": "./src/index.js",
    "files": [
        "./src"
    ],
    "license": "Apache-2.0",
    "scripts": {
        "poshify": "npx poshify",
        "pretty": "npx prettier --write .",
        "lint": "eslint ./src/",
        "lint:fix": "eslint --fix ./src/",
        "test": "npx vitest --run",
        "build": "npm run poshify && npm run pretty && npm run lint:fix && npm run lint"
    },
    "keywords": [
        "graphemes",
        "string",
        "unicode",
        "de-duplication",
        "locale",
        "intl.segmenter"
    ],
    "devDependencies": {
        "eslint": "^9.36.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "poshify": "^1.0.0",
        "prettier": "^3.6.2",
        "vitest": "^3.2.4"
    }
}
